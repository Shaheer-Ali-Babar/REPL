class Number:
    def __init__(self, value):
        self.value = value

class Variable:
    def __init__(self, name):
        self.name = name

class BinaryOp:
    def __init__(self, left, op, right):
        self.left = left
        self.op = op
        self.right = right

class LetStatement:
    def __init__(self, name, expr):
        self.name = name
        self.expr = expr

class PrintStatement:
    def __init__(self, expr):
        self.expr = expr
